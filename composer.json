{
    "name": "reliv/deploy",
    "description": "A php deploy helper.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "reliv",
        "deploy"
    ],
    "homepage": "http://www.reliv.com/",
    "authors": [
        {
            "name": "Westin Shafer",
            "email": "wshafer@relivinc.com",
            "homepage": "http://www.reliv.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wshafer/git.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "~2.6",
        "zendframework/zend-config": "~2.3",
        "symfony/event-dispatcher": "~2.6",
        "symfony/process": "~2.6",
        "symfony/monolog-bridge": "~2.6",
        "reliv/git": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Reliv\\Deploy\\": "src/"
        },
        "classmap": [
            "./"
        ]
    }
}
